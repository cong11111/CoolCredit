<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical">

        <ImageView
            android:id="@+id/iv_back"
            android:layout_width="@dimen/dp_20"
            android:layout_height="@dimen/dp_29"
            android:layout_marginLeft="@dimen/dp_11"
            android:layout_marginTop="@dimen/dp_6"
            android:layout_marginBottom="@dimen/dp_6"
            android:padding="@dimen/dp_2"
            android:src="@mipmap/btn_back_black" />

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingLeft="@dimen/dp_16"
                android:paddingRight="@dimen/dp_16">

                <ImageView
                    android:layout_width="@dimen/dp_300"
                    android:layout_height="@dimen/dp_42"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/dp_18"
                    android:background="@drawable/bg_steps2" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/dp_20"
                    android:text="@string/str_about_you_referee"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_20" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_30"
                    android:text="@string/str_first_referee_s"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_20" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_20"
                    android:text="@string/str_referee_full_leagal_name"
                    android:textColor="@color/color_ff999999"
                    android:textSize="@dimen/sp_16" />

                <TextView
                    android:id="@+id/et_first_contact_name"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_34"
                    android:background="@drawable/edittext_selector"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_18" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_20"
                    android:text="@string/str_referee_mobile_number"
                    android:textColor="@color/color_ff999999"
                    android:textSize="@dimen/sp_16" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/btn_login_frame_tips_selector"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/et_first_contact_mobile"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_34"
                        android:background="@drawable/edittext_selector"
                        android:gravity="center_vertical"
                        android:inputType="number"
                        android:maxLength="14"
                        android:singleLine="true"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_18" />
                    <ImageView
                        android:id="@+id/iv_first_contact_mobile_clear"
                        android:layout_width="@dimen/dp_16"
                        android:layout_height="@dimen/dp_16"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/dp_15"
                        android:layout_toLeftOf="@id/iv_first_contact_mobile"
                        android:clickable="true"
                        android:padding="@dimen/dp_2"
                        android:src="@drawable/ic_edittext_clear"
                        android:visibility="gone" />


                    <ImageView
                        android:id="@+id/iv_first_contact_mobile"
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_20"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:paddingLeft="@dimen/dp_4"
                        android:paddingTop="@dimen/dp_2"
                        android:paddingBottom="@dimen/dp_2"
                        android:src="@mipmap/ic_directory" />

                </RelativeLayout>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_20"
                    android:text="@string/str_ralationship"
                    android:textColor="@color/color_ff999999"
                    android:textSize="@dimen/sp_16" />

                <org.angmarch.views.NiceSpinner
                    android:id="@+id/spinner_first_ralationship"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_10020"
                    android:layout_marginTop="@dimen/dp_5"
                    android:gravity="start"
                    android:hint="@string/str_choose_from_list"
                    android:textColor="@color/color_ffcccccc"
                    android:textSize="@dimen/sp_18"
                    app:backgroundSelector="@drawable/background_selector" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_30"
                    android:text="@string/str_second_referee"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_20" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_20"
                    android:text="@string/str_referee_full_leagal_name"
                    android:textColor="@color/color_ff999999"
                    android:textSize="@dimen/sp_16" />

                <TextView
                    android:id="@+id/et_second_contact_name"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_34"
                    android:background="@drawable/edittext_selector"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_18" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_20"
                    android:text="@string/str_referee_mobile_number"
                    android:textColor="@color/color_ff999999"
                    android:textSize="@dimen/sp_16" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/btn_login_frame_tips_selector"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/et_second_contact_mobile"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_34"
                        android:inputType="number"
                        android:maxLength="14"
                        android:singleLine="true"
                        android:background="@drawable/edittext_selector"
                        android:gravity="center_vertical"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_18" />

                    <ImageView
                        android:id="@+id/iv_second_contact_mobile_clear"
                        android:layout_width="@dimen/dp_16"
                        android:layout_height="@dimen/dp_16"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/dp_15"
                        android:layout_toLeftOf="@id/iv_second_contact_mobile"
                        android:clickable="true"
                        android:padding="@dimen/dp_2"
                        android:src="@drawable/ic_edittext_clear"
                        android:visibility="gone" />


                    <ImageView
                        android:id="@+id/iv_second_contact_mobile"
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_20"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:paddingLeft="@dimen/dp_4"
                        android:paddingTop="@dimen/dp_2"
                        android:paddingBottom="@dimen/dp_2"
                        android:src="@mipmap/ic_directory" />

                </RelativeLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_20"
                    android:text="@string/str_ralationship"
                    android:textColor="@color/color_ff999999"
                    android:textSize="@dimen/sp_16" />

                <org.angmarch.views.NiceSpinner
                    android:id="@+id/spinner_second_ralationship"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_5"
                    android:gravity="start"
                    android:layout_marginLeft="@dimen/dp_10020"
                    android:hint="@string/str_choose_from_list"
                    android:textColor="@color/color_ffcccccc"
                    android:textSize="@dimen/sp_18"
                    app:backgroundSelector="@drawable/background_selector" />

                <LinearLayout
                    android:id="@+id/ll_more_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    tools:visibility="visible">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_30"
                        android:text="@string/str_three_referee"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_20" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_20"
                        android:text="@string/str_referee_full_leagal_name"
                        android:textColor="@color/color_ff999999"
                        android:textSize="@dimen/sp_16" />

                    <TextView
                        android:id="@+id/et_three_contact_name"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_34"
                        android:background="@drawable/edittext_selector"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_18" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_20"
                        android:text="@string/str_referee_mobile_number"
                        android:textColor="@color/color_ff999999"
                        android:textSize="@dimen/sp_16" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/btn_login_frame_tips_selector"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/et_three_contact_mobile"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_34"
                            android:inputType="number"
                            android:maxLength="14"
                            android:singleLine="true"
                            android:background="@drawable/edittext_selector"
                            android:gravity="center_vertical"
                            android:textColor="@color/color_333333"
                            android:textSize="@dimen/sp_18" />

                        <ImageView
                            android:id="@+id/iv_three_contact_mobile_clear"
                            android:layout_width="@dimen/dp_16"
                            android:layout_height="@dimen/dp_16"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dp_15"
                            android:layout_toLeftOf="@id/iv_three_contact_mobile"
                            android:clickable="true"
                            android:padding="@dimen/dp_2"
                            android:src="@drawable/ic_edittext_clear"
                            android:visibility="gone" />


                        <ImageView
                            android:id="@+id/iv_three_contact_mobile"
                            android:layout_width="@dimen/dp_20"
                            android:layout_height="@dimen/dp_20"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:paddingLeft="@dimen/dp_4"
                            android:paddingTop="@dimen/dp_2"
                            android:paddingBottom="@dimen/dp_2"
                            android:src="@mipmap/ic_directory" />

                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_20"
                        android:text="@string/str_ralationship"
                        android:textColor="@color/color_ff999999"
                        android:textSize="@dimen/sp_16" />

                    <org.angmarch.views.NiceSpinner
                        android:id="@+id/spinner_three_ralationship"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_5"
                        android:gravity="start"
                        android:layout_marginLeft="@dimen/dp_10020"
                        android:hint="@string/str_choose_from_list"
                        android:textColor="@color/color_ffcccccc"
                        android:textSize="@dimen/sp_18"
                        app:backgroundSelector="@drawable/background_selector" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_30"
                        android:text="@string/str_four_referee"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_20" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_20"
                        android:text="@string/str_referee_full_leagal_name"
                        android:textColor="@color/color_ff999999"
                        android:textSize="@dimen/sp_16" />

                    <TextView
                        android:id="@+id/et_four_contact_name"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_34"
                        android:background="@drawable/edittext_selector"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_18" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_20"
                        android:text="@string/str_referee_mobile_number"
                        android:textColor="@color/color_ff999999"
                        android:textSize="@dimen/sp_16" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/btn_login_frame_tips_selector"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/et_four_contact_mobile"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_34"
                            android:inputType="number"
                            android:maxLength="14"
                            android:singleLine="true"
                            android:background="@drawable/edittext_selector"
                            android:gravity="center_vertical"
                            android:textColor="@color/color_333333"
                            android:textSize="@dimen/sp_18" />

                        <ImageView
                            android:id="@+id/iv_four_contact_mobile_clear"
                            android:layout_width="@dimen/dp_16"
                            android:layout_height="@dimen/dp_16"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dp_15"
                            android:layout_toLeftOf="@id/iv_four_contact_mobile"
                            android:clickable="true"
                            android:padding="@dimen/dp_2"
                            android:src="@drawable/ic_edittext_clear"
                            android:visibility="gone" />


                        <ImageView
                            android:id="@+id/iv_four_contact_mobile"
                            android:layout_width="@dimen/dp_20"
                            android:layout_height="@dimen/dp_20"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:paddingLeft="@dimen/dp_4"
                            android:paddingTop="@dimen/dp_2"
                            android:paddingBottom="@dimen/dp_2"
                            android:src="@mipmap/ic_directory" />

                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_20"
                        android:text="@string/str_ralationship"
                        android:textColor="@color/color_ff999999"
                        android:textSize="@dimen/sp_16" />

                    <org.angmarch.views.NiceSpinner
                        android:id="@+id/spinner_four_ralationship"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_5"
                        android:gravity="start"
                        android:layout_marginLeft="@dimen/dp_10020"
                        android:hint="@string/str_choose_from_list"
                        android:textColor="@color/color_ffcccccc"
                        android:textSize="@dimen/sp_18"
                        app:backgroundSelector="@drawable/background_selector" />
                </LinearLayout>

                <TextView
                    android:id="@+id/tv_more_Continue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/dp_20"
                    android:text="@string/str_continue_to_fill_in"
                    android:textColor="@color/color_ff666666"
                    android:textSize="@dimen/sp_16" />

                <Button
                    android:id="@+id/btn_next"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_42"
                    android:layout_marginTop="@dimen/dp_30"
                    android:layout_marginBottom="@dimen/dp_20"
                    android:background="@drawable/btn_login_selector"
                    android:enabled="true"
                    android:text="@string/str_next"
                    android:textColor="@color/color_FFFFFF"
                    android:textSize="@dimen/sp_15" />
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
